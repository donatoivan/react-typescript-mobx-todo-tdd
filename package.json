{
  "name": "react-typescript-mobx-todo-tdd",
  "version": "1.0.0",
  "description": "A simple react, typescript, and mobx todo app done with tdd.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest --coverage",
    "build": "tsc && webpack --progress --config webpack.prod.config.js"
  },
  "author": "Ivan Donato",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/donatoivan/react-typescript-mobx-todo-tdd"
  },
  "dependencies": {
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.2.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.24.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.1.6",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "4.0.3",
    "@types/testing-library__jest-dom": "^5.9.2",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.0",
    "source-map-loader": "^1.0.1",
    "ts-import-plugin": "^1.6.5",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  }
}
